<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <!--Time as hour to run the application-->
    <add key="hour" value="1" />
    <!--How many times we want the cursor move per minute-->
    <add key="frequency" value="60" />
  </appSettings>
</configuration>